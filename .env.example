# Docker PostgreSQL Configuration
POSTGRES_DB=paymentdb
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_PORT=5433

# Kafka Configuration
Kafka__BootstrapServers=localhost:9092

# Payment Gateway Configuration
PaymentGateways__Stripe__WebhookSecret=your_stripe_webhook_secret_here
PaymentGateways__Stripe__SecretKey=your_stripe_secret_key_here

# JWT Configuration
Jwt__SecretKey=your_jwt_secret_key_here_min_32_chars
Jwt__Issuer=https://your-auth-service.com
Jwt__Audience=payment-service
